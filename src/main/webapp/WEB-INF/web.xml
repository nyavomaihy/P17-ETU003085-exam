<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
    http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
    version="4.0">

<servlet>
    <servlet-name>formulaire_presvision</servlet-name>
    <servlet-class>com.itu.Hello.PrevisionServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>formulaire_presvision</servlet-name>
    <url-pattern>/formulaire_prevision</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>depense</servlet-name>
    <servlet-class>com.itu.Hello.DepenseServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>depense</servlet-name>
    <url-pattern>/depense</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>faireDepense</servlet-name>
    <servlet-class>com.itu.Hello.FaireDepenseServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>faireDepense</servlet-name>
    <url-pattern>/faireDepense</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>etat</servlet-name>
    <servlet-class>com.itu.Hello.EtatServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>etat</servlet-name>
    <url-pattern>/etat</url-pattern>
</servlet-mapping>
</web-app>

